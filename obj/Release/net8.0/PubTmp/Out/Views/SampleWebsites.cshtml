@using Umbraco.Cms.Web.Common.PublishedModels;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
	var content = Model.Content as Umbraco.Cms.Core.Models.PublishedContent.IPublishedContent;
	Umbraco.Cms.Core.Models.Blocks.BlockListModel SampleWebsites = ((Umbraco.Cms.Web.Common.PublishedModels.SampleWebsites)content).List_site;

}
@if (SampleWebsites.Count > 0)
{
	<div class="row container">

		@for (int i = 0; i < SampleWebsites.Count; i++)
		{
			Umbraco.Cms.Web.Common.PublishedModels.List_site_build SampleWebsite = ((Umbraco.Cms.Core.Models.Blocks.BlockListItem<Umbraco.Cms.Web.Common.PublishedModels.List_site_build>)(new System.Collections.Generic.List<Umbraco.Cms.Core.Models.Blocks.BlockListItem>(SampleWebsites)[i])).Content;


			<a target="_blank" href="@(SampleWebsite.Url_site.Url)" class="col-12 col-md-4 card back_img  SampleWebsite" style="background-image:url(@(((Umbraco.Cms.Core.Models.MediaWithCrops<Umbraco.Cms.Web.Common.PublishedModels.Image>)SampleWebsite.Site_img).Content.UmbracoFile))">

			</a>
		}
	</div>
}
